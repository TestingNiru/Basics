package Com.PlaywithNumbers;

public class countString {

	public static void main(String[] args) {
		String str= "java is my java";
		String s[]=str.split(" ");
		int count=0;
		for(int i=0;i<s.length;i++) {
			for(int j=i+1;j<s.length;j++) {
				if(s[i].equals(s[j])) {
					count++;
				
					System.out.println(s[i]+"="+count);
					
				}
			}
		
		
	}

}}
