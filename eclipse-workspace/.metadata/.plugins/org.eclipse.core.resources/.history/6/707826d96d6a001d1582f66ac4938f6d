package javaprgms;

public class chkchr {
public static void isAlphabet(char c) {
	
	if(c>='a'&& c<='z'||c>='A'&& c<='Z') {
		System.out.println(c+" "+"is an alphabet");}
	else {
		System.out.println(c+" "+"is not an alphabet");
	
}}
	public static void main(String[] args) {
		isAlphabet('y');
	}
	}
		

	


