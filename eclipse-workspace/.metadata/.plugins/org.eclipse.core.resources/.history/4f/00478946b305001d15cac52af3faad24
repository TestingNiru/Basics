package Com.PlaywithNumbers;

public class prm {
	public static boolean prim(int num) {
		if(num==1) {
			return false;}
			for(int i=2;i<num;i++) {
				if(num%i==0) {
					return false;
				}
				
			}
			return true;
		}
	public static void getprm(int num) {
	
		for(int i=2;i<=num;i++) {
			if(prim(i)) {
				System.out.println(i);
			}
		}
		;
	}
		
		public static void main(String[] args) {
getprm(7);
	}

}
