package Com.PlaywithNumbers;

public class prm {
	public static boolean prime(int num) {
		if(num==1) {
			return false;
		}
		for(int i=2;i<num;i++) {
			if(num%i==0) {
				return false;
			}
	
		}
		return true;
	}

	public static void main(String[] args) {
	
getprm(7);
	}

	public static void getprm(int num) {
		int count=0;
		for(int i=2;i<=num;i++) {
		if(prime(i)) {
			count++;}
		}
		System.out.println(count);
	}
}
