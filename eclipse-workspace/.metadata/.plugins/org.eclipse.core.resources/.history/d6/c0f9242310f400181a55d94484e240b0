package pack2;

public class A {
	A(){
		System.out.println("ctt of A without arg");
	}
	A(int i){
		System.out.println("ctt of A");
	}
void	test(){
	System.out.println("test1 of A");
		
	}

}
class B extends A{
	B(){
		super(10);
		System.out.println("ctt of B");
	}
	void test2() {
		System.out.println("test1 of B");	
	}
}