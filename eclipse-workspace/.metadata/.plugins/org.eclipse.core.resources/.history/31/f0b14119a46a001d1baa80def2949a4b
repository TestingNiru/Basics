package javaprgms;

public class amstng {
	public static void isAmstrng(int num) {
		int cube=0;
		int r;
		int t;
		t=num;
		while(num>0) {
			r=num%10;
			cube=cube+r*r*r;
			num=num/10;
			
		}
		if(t==cube) {
			System.out.println("num is amstrong num");
		}
		else {
			System.out.println("num is not an amstrong num");
		}
	}

	public static void main(String[] args) {
		isAmstrng(153);

	}

}
