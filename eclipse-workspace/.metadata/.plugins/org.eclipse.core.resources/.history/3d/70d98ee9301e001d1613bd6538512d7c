
public class upperlwrchrCnt {

	public static void main(String[] args) {
String s="WELCOME tojava";
int lwr=0;
int upr=0;
for(int i=0;i<s.length();i++) {
	if (s.charAt(i)>='a'&& s.charAt(i)<='z') {
		lwr++;
	}
	else if (s.charAt(i)>='A' && s.charAt(i)<='Z' ) {
		upr++;
			}
	else {System.out.println("invalid char");
		
	}
}

System.out.println("lwr count"+lwr);
System.out.println("upr count"+upr);
	}

}
