package javaprgms;

import java.util.ArrayList;

public class Arlst {

	public static void main(String[] args) {
	ArrayList<Integer> arl=new ArrayList<Integer>();
	arl.add(100);
	arl.add(200);
	arl.add(300);
	arl.add(400);
	arl.add(500);
	int size=arl.size();
	System.out.println(size);
	for(int i=0;i<size;i++) {
		System.out.println(arl.get(i));
	}
	}

}
