package Com.PlaywithNumbers;

public class revNum {
	public static void palindrm(int num) {
		int t=num;
		int rev=0;
		int r=0;
		while(num!=0) {
			r=num%10;
			rev=rev*10+r;
			num=num/10;
	}
	if	(t==rev) {
		System.out.println("palindrm");
	}
	else {System.out.println("not palindrm");
	}
	}

	public static void main(String[] args) {
palindrm(131);
	}

	}


