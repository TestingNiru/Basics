package JavaOOPS;

class car{
	
	int model;
	String type;
	float price;
	
	
	
	public void show() {
	
		System.out.println("Model No : " );
		System.out.println("Model Type :");
		System.out.println("Model Price :");
		
	}
}

public class TestDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Java Demo");
		
		car c1 = new car();
		c1.show();

	}

}
