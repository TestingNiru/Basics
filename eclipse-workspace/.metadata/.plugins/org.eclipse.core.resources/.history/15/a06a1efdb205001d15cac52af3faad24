package Com.PlaywithNumbers;

public class prm {
	public static boolean prim(int num) {
		if(num==1) {
			return false;}
			for(int i=2;i<num;i++) {
				if(num%i==0) {
					return false;
				}
				
			}
			return true;
		}
		
	

	public static void main(String[] args) {
		
 System.out.println(prim(12));
	}

}
