package javaprgms;

public class lrgstnmr {

	public static void main(String[] args) {
	int x=100;
	int y=320;
	int z=128;
	if(x>y && x>z) {
		System.out.println(x+" "+"is the largest");
	}
	else if(y>x && y>z) {
		System.out.println(y+" "+"is the largest");
	}
	else
		System.out.println(z+" "+"is the largest");
	}

}
