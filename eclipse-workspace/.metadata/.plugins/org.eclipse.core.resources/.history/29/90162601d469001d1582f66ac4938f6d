package Com.PlaywithNumbers;

public class pldrm {

	public static void isPalidrm(int num) {
		int sum=0;
		int t;
		t=num;
		while(num!=0) {
			int r=num%10;
			sum=sum*10+r;
			num=num/10;
		}
		if(t==sum) {
			System.out.println("No is paldrm");
		}
		else 
			System.out.println("No is not paldrm");
			
		
	}
	
	public static void main(String[] args) {
	isPalidrm(1211);

	}

}
